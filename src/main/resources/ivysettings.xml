<ivysettings>
    <property name="ivy.cache.ttl.default" value="1d"/>
	<settings defaultResolver="walkmod-core" />
	<configurations defaultconfmapping="compile->compile">
	</configurations>
	<resolvers>
		<chain name="walkmod-core">

			<ibiblio name="maven2" m2compatible="true"
				pattern="https://repo1.maven.org/maven2/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]" />
			<ibiblio name="jboss" root="https://repository.jboss.com/maven2"
				m2compatible="true" usepoms="false" />
			<ibiblio name="springsource-external" m2compatible="true"
				root="https://repository.springsource.com/maven/bundles/external" />
			<ibiblio name="springsource-release" m2compatible="true"
				root="https://repository.springsource.com/maven/bundles/release" />
			<ibiblio name="springio-release" m2compatible="true"
				root="https://repo.spring.io/libs-release-remote/" />
		</chain>
	</resolvers>
</ivysettings>
